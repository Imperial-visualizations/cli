{
    "name": "{{projectName}}",
    "version": "0.1.0",
    "private":true,
    "scripts": {
      "serve": "vue-cli-service serve",
      "build": "vue-cli-service build",
      "postbuild": "./postbuild.js",
      "lint": "vue-cli-service lint"
    },
    "keywords": [
      "impvis"
    ],
    "author": "Robert King",
    "license": "BSD-3-Clause",
    "dependencies": {
        "core-js": "^3.6.5",
        "vue": "^2.6.11",
        "@impvis/components":"*"
    },
    "devDependencies": {
        "vue-template-compiler": "^2.6.11",
        "@vue/cli-service":"^4.5.3",
        "archiver":"^5.0.2"
    }
    {{#if eslint}}
    ,"eslintConfig": {
      "root": true,
      "env": {
        "node": true
      },
      "extends": [
          "plugin:vue/essential",
          "eslint:recommended"
      ],
      "parserOptions": {
        {{#if babel}}
        "parser": "babel-eslint"
        {{/if}}
      },
      "rules": {}
    }
    {{/if}}
  }
  
